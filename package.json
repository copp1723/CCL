  "description": "Complete Car Loans - AI Agent System for Lead Generation and Customer Engagement",
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "build": "npx vite build && npx esbuild server/index-robust.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --minify",
    "start": "cross-env NODE_ENV=production node dist/index-robust.js",
    "check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "tsx test-integration.js",
    "test:e2e": "playwright test",
    "clean": "rm -rf dist node_modules/.cache",
    "lint": "eslint . --max-warnings 500",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:migrate": "drizzle-kit migrate",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "mvp:sftp": "tsx server/services/sftp-ingestor.ts",
    "mvp:abandonment": "tsx server/jobs/abandonment-detector.ts",
    "mvp:outreach": "tsx server/jobs/outreach-orchestrator.ts",
    "mvp:health": "tsx scripts/health-check.ts",
    "test:sms": "tsx scripts/test-sms.ts",
    "test:boberdoo": "tsx scripts/test-boberdoo.ts",
    "audit": "npm audit --audit-level moderate",
    "bundlesize": "echo 'Bundle size monitoring integrated with Vite build process'"
  },
